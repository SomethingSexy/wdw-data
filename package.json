    "service:waittimes": "ts-node ./scripts/waittimes.ts",
    "test": "mocha --opts test/mocha.opts",
    "version": "npm run build && git add -A modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://somethingsexy@github.com/SomethingSexy/wdw-data.git"
