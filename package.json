    "@types/mocha": "^2.2.48",
    "@types/moment-holiday": "^1.5.0",
    "@types/sequelize": "^4.27.24",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "mocha": "^5.1.1",
    "ts-loader": "^3.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.10.0",
